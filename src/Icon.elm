module Icon exposing (elm, github, logo)

import Html.Attributes as HA
import Svg exposing (..)
import Svg.Attributes exposing (..)


logo : Int -> Svg msg
logo length =
    svg
        [ viewBox "0 0 10 10"
        , height (String.fromInt length)
        , HA.attribute "role" "img"
        ]
        [ Svg.path
            [ fill "none"
            , stroke "#fff"
            , d "M 0 0 h 10 v 10 H 0 z L 5 5 M 0 10 L 10 0"
            ]
            []
        ]


